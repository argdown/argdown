{
  "name": "@argdown/image-export",
  "version": "1.7.5",
  "description": "PNG, JPG and WEBP export for Argdown",
  "main": "dist/index.js",
  "module": "dist-esm/index.js",
  "scripts": {
    "build": "npm run clean && npm run build:src && npm run build:esm",
    "build:esm": "tsc -m es6 --target ES2022 --outDir dist-esm",
    "build:src": "tsc",
    "test": "mocha -r ts-node/register --full-trace --bail --require source-map-support/register ./test/**/*.spec.ts",
    "clean": "rimraf \"dist/*\" --glob && rimraf \"dist-esm/*\" --glob"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/christianvoigt/argdown/tree/master/packages/argdown-png-export"
  },
  "keywords": [
    "Argdown",
    "png"
  ],
  "engines": {
    "node": ">= 22.11.0"
  },
  "author": "Christian Voigt",
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^5.2.1",
    "@types/mocha": "^10.0.10",
    "chai": "^5.2.0",
    "mocha": "11.1.0",
    "rimraf": "^6.0.1",
    "ts-node": "10.9.2",
    "typescript": "5.8.2"
  },
  "dependencies": {
    "@argdown/core": "^1.7.5",
    "@argdown/node": "^1.7.5",
    "lodash.defaultsdeep": "^4.6.1",
    "svg-to-img": "^2.0.9"
  }
}
