{
  "name": "@argdown/sandbox",
  "version": "1.7.10",
  "private": true,
  "description": "An Argdown editor that runs in the browser",
  "author": "Christian Voigt <1pxsolidblack@googlemail.com>",
  "scripts": {
    "serve": "npm run copy-examples && vue-cli-service serve --open",
    "build": "npm run copy-examples && vue-cli-service build --dest ../../docs/.vuepress/dist/sandbox",
    "lint": "vue-cli-service lint",
    "copy-examples": "rimraf \"./public/examples\" && copyfiles -f \"../../examples/*.argdown\" ./public/examples"
  },
  "dependencies": {
    "@argdown/codemirror-mode": "^1.7.4",
    "@argdown/core": "^1.7.5",
    "@argdown/map-views": "^1.7.10",
    "axios": "^1.7.9",
    "file-saver": "^2.0.5",
    "vue": "^3.4.28",
    "vue-router": "^4.3.2",
    "pinia": "^2.2.1",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@vue/cli-plugin-babel": "^5.0.8",
    "@vue/cli-plugin-eslint": "^5.0.8",
    "@vue/cli-service": "^5.0.8",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/compiler-sfc": "^3.4.28",
    "@babel/eslint-parser": "^7.24.1",
    "babel-plugin-dynamic-import-node": "^2.3.3",
    "copyfiles": "^2.4.1",
    "css-loader": "^7.1.2",
    "eslint": "^8.57.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-vue": "^9.25.0",
    "sass": "^1.72.0",
    "prettier": "^3.3.3",
    "raw-loader": "^4.0.2",
    "rimraf": "^6.0.1",
    "sass-loader": "^14.1.1",
    "webpack-bundle-analyzer": "^4.10.1",
    "path-browserify": "^1.0.1"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "sideEffects": [
    "./src/main.js",
    "./src/store.js",
    "*.css",
    "*.scss",
    "*.vue"
  ],
  "keywords": [
    "argumentation",
    "argument mapping",
    "Argdown",
    "Editor"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/christianvoigt/argdown/tree/master/packages/argdown-sandbox"
  }
}
