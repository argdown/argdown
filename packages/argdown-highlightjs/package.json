{
  "name": "@argdown/highlightjs",
  "version": "1.7.5",
  "description": "Argdown language definition for highlightjs",
  "main": "dist/index.js",
  "module": "dist-esm/index.js",
  "files": [
    "dist/",
    "dist-esm/"
  ],
  "scripts": {
    "sandbox": "parcel ./test/sandbox.html",
    "build": "npm run clean && npm run build:src && npm run build:esm && node copy-css.js",
    "build:esm": "tsc -m es6 --target ES2018 --outDir dist-esm",
    "build:src": "tsc",
    "clean": "rimraf \"dist/*\" --glob && rimraf \"dist-esm/*\" --glob",
    "copy-css": ""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/christianvoigt/argdown/tree/master/packages/argdown-highlightjs"
  },
  "keywords": [
    "Argdown",
    "highlightjs"
  ],
  "engines": {
    "node": ">= 22.11.0"
  },
  "author": "Christian Voigt",
  "license": "MIT",
  "devDependencies": {
    "@parcel/config-default": "^2.14.4",
    "@parcel/optimizer-cssnano": "^2.14.4",
    "@parcel/optimizer-htmlnano": "^2.14.4",
    "@parcel/packager-css": "^2.14.4",
    "@parcel/packager-html": "^2.14.4",
    "@parcel/transformer-css": "^2.14.4",
    "@parcel/transformer-html": "^2.14.4",
    "@parcel/transformer-postcss": "^2.14.4",
    "@parcel/transformer-posthtml": "^2.14.4",
    "@parcel/transformer-typescript-tsc": "^2.14.4",
    "@parcel/validator-typescript": "^2.14.4",
    "highlight.js": "11.11.1",
    "parcel": "^2.14.4",
    "postcss": "8.5.3",
    "rimraf": "^6.0.1",
    "ts-node": "10.9.2",
    "typescript": "5.8.3"
  }
}
